//利用指针数组输出单位矩阵
int main()
{
	int line1[] = { 1,0,0 };
	int line2[] = { 0,1,0 };
	int line3[] = { 0,0,1 };
	int* p_line[3];
	p_line[0] = line1;
	p_line[1] = line2;
	p_line[2] = line3;
	printf("Matrix test:\n");
	int i = 0;
	for (i = 0; i < 3; i++)
	{
		int j = 0;
		for (j = 0; j < 3; j++)
		{
			printf("%d ", p_line[i][j]);
		}
		printf("\n");
	}

	return 0;
}
