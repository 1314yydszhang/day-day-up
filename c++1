#define _CRT_SECURE_NO_WARNINGS  1

#include<iostream>
using namespace std;
int main()
{
	cout << "hello world" << endl;
	//默认情况 输出小数，显示6位有效数字
	//float 4 double 8
	//short 2 int 4 long 4/8 long long8
	float b = 3.114f;
	float c1 = 3e2;//3*10^2
	float c2 = 3e-2;//3*0.1^2
	int a = 10;
	cout << "a=" << a << endl;

	char a1 = 'a';
	//a-97
	//A-65
	cout << "a1=" << (int)a1 << endl;
	system("pause");
	
	return 0;
}
